<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" spacing="10.0"
         type="VBox" xmlns="http://javafx.com/javafx/8.0.162-ea">
    <children>
        <Label text="Übersicht"/>
        <ListView fx:id="overviewList" prefHeight="200.0"
                  prefWidth="200.0"/>
        <Button fx:id="deleteHistory" mnemonicParsing="false"
                onAction="#onDeleteHistoryClick" text="Ergebnisse löschen"/>
    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</fx:root>
